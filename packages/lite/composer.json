{
    "name": "greenter/lite",
    "description": "Greenter lite version",
    "keywords": ["greenter", "sunat", "xml", "facturacion-electronica"],
    "homepage": "https://greenter.dev/",
    "license": "MIT",
    "authors": [
        {
            "name": "Giancarlos Salas",
            "email": "giansalex@gmail.com"
        }
    ],
    "support": {
        "forum": "https://community.greenter.dev/"
    },
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "greenter/xmldsig": "^5.0",
        "greenter/core": "^4.4",
        "greenter/ws": "^4.4",
        "greenter/xml": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Greenter\\": "src/Greenter/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.4-dev"
        }
    }
}
