	<xsd:element name="SUNATShipment" type="SUNATShipmentType" />
	<xsd:element name="SUNATShipmentStage" type="SUNATShipmentStageType" />
	<xsd:element name="SUNATTransportMeans" type="SUNATTransportMeansType" />
