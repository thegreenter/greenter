	<!-- IMR 2016-08-26 Factura guÃ­a -->
	<xsd:element name="OrderReference" type="OrderReferenceType" />
	<xsd:element name="DriverPerson" type="DriverPersonType" />
	<xsd:element name="DocumentReference" type="DocumentReferenceType" />
	<xsd:element name="Shipment" type="ShipmentType" />
	<xsd:element name="ShipmentStage" type="ShipmentStageType" />
	<xsd:element name="OrderTypeCode" type="udt:CodeType" />
	
	<xsd:complexType name="OrderReferenceType">
		<xsd:sequence>
			<xsd:element ref="cbc:ID" minOccurs="1"	maxOccurs="1" />
			<xsd:element ref="OrderTypeCode" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="DocumentReference" minOccurs="0" maxOccurs="1" />										
		</xsd:sequence>
	</xsd:complexType>
		
	<xsd:complexType name="DriverPersonType">
		<xsd:sequence>
			<xsd:element ref="cbc:ID" minOccurs="0" maxOccurs="1" />
		</xsd:sequence>			
	</xsd:complexType>
		
	<xsd:complexType name="DocumentReferenceType">
		<xsd:complexContent>
			<xsd:extension base="cac:DocumentReferenceType">
				<xsd:sequence>
					<xsd:element ref="cac:IssuerParty" minOccurs="0"/>					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>				
	</xsd:complexType>
	
	<xsd:complexType name="ShipmentType">
		<xsd:sequence>
			<xsd:element ref="cbc:HandlingCode" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:GrossWeightMeasure" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="ShipmentStage" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cac:Delivery" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cac:TransportHandlingUnit" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="cac:OriginAddress" minOccurs="0" maxOccurs="1" />						
		</xsd:sequence>		
	</xsd:complexType>
	
	<xsd:complexType name="ShipmentStageType">
		<xsd:sequence>
			<xsd:element ref="cbc:TransportModeCode" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cac:TransitPeriod" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cac:CarrierParty" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="cac:TransportMeans" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="DriverPerson" minOccurs="0" maxOccurs="unbounded" />									
		</xsd:sequence>		
	</xsd:complexType>
	
	
	<xsd:complexType name="SUNATEmbededDespatchAdviceType">
		<xsd:sequence>
			<!-- Js. 030117 definicion tags PASS PAS20165E210300249-->		
			<xsd:element ref="cac:DeliveryAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:OriginAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="SUNATCarrierParty" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="DriverParty" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element ref="SUNATRoadTransport" minOccurs="0"
				maxOccurs="unbounded" />
			<!--Tipo de transporte -->
			<xsd:element ref="cbc:TransportModeCode" minOccurs="0"
				maxOccurs="1" />
			<!--Peso Neto de la Factura -->
			<xsd:element ref="cbc:GrossWeightMeasure" minOccurs="0"
				maxOccurs="1" />
			<!-- Fin Js. 030117-->				
				
			<xsd:element ref="OrderReference" minOccurs="0"	maxOccurs="1" />
			<xsd:element ref="cac:DeliveryCustomerParty" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="Shipment" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:MarkAttentionIndicator" minOccurs="0" maxOccurs="1" />					
								
		</xsd:sequence>		
	</xsd:complexType>
	
	
	<!-- Fin IMR 2016-08-26 -->
	<xsd:complexType name="DriverPartyType">
		<xsd:sequence>
			<xsd:element ref="cbc:CustomerAssignedAccountID"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:AdditionalAccountID" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="cac:Party" minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATCarrierPartyType">
		<xsd:sequence>
			<xsd:element ref="cbc:CustomerAssignedAccountID"
				minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:AdditionalAccountID" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="cbc:TransportAuthorizationCode"
				minOccurs="0" />
			<xsd:element ref="OutsourcedIndicator" minOccurs="0" />
			<xsd:element ref="cac:Party" minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATRoadTransportType">
		<xsd:complexContent>
			<xsd:extension base="cac:RoadTransportType">
				<xsd:sequence>
					<xsd:element ref="cbc:TransportMeansTypeCode"
						minOccurs="0" />
					<xsd:element ref="cbc:TransportAuthorizationCode"
						minOccurs="0" />
					<xsd:element ref="SemiTrailer" minOccurs="0" maxOccurs="5" />
					<xsd:element ref="cbc:BrandName" minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SUNATTransportMeansType">
		<xsd:complexContent>
			<xsd:extension base="cac:TransportMeansType">
				<xsd:sequence>
					<xsd:element ref="DriverParty" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element ref="SUNATRoadTransport" minOccurs="0"
						maxOccurs="unbounded" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SUNATShipmentStageType">
		<xsd:sequence>
			<xsd:element ref="cbc:ID" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="ShipmentStageTypeCode" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:TransportModeCode" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:TransportMeansTypeCode" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="SUNATCarrierParty" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="SUNATTransportMeans" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:OriginAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:DeliveryAddress" minOccurs="0"
				maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATShipmentType">
		<xsd:sequence>
			<xsd:element ref="cbc:GrossWeightMeasure" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:DeliveryAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:OriginAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="OldDeliveryAddress" minOccurs="0" />
			<xsd:element ref="OldOriginAddress" minOccurs="0" />
			<xsd:element ref="SUNATShipmentStage" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATDespatchLineType">
		<xsd:sequence>
			<xsd:element ref="cbc:ID" minOccurs="1" maxOccurs="1" />
			<xsd:element ref="cbc:UUID" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:Note" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:LineStatusCode" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:DeliveredQuantity" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:BackorderQuantity" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:BackorderReason" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:OutstandingQuantity" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:OutstandingReason" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:OversupplyQuantity" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:OrderLineReference" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="cac:DocumentReference" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="cac:Item" minOccurs="1" maxOccurs="1" />
			<xsd:element ref="cbc:GrossWeightMeasure" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:Shipment" minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DeliveryCarrierDateType">
		<xsd:simpleContent>
			<xsd:extension base="udt:DateType" />
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="SUNATFiscalPathType">
		<xsd:sequence>
			<xsd:element ref="cbc:ID" minOccurs="1" maxOccurs="1" />
			<xsd:element ref="cbc:Name" minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATTransactionType">
		<xsd:sequence>
			<xsd:element ref="cbc:ID" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<!-- ===== End Despatch advice elements ===== -->
