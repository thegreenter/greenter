			</xsd:element>
			<xsd:element ref="SUNATTotalCashed">
				<xsd:annotation>
					<xsd:documentation>
						<ccts:Component>
							<ccts:ComponentType>BBIE</ccts:ComponentType>
							<ccts:DictionaryEntryName>Monto total a cobrar incluida la percepcion
							</ccts:DictionaryEntryName>
							<ccts:Definition>Identifies the Consolidated Invoice Line.
							</ccts:Definition>
							<ccts:Cardinality>1</ccts:Cardinality>
							<ccts:ObjectClass>Consolidated Invoice Line</ccts:ObjectClass>
							<ccts:PropertyTerm>Identifier</ccts:PropertyTerm>
							<ccts:RepresentationTerm>Identifier</ccts:RepresentationTerm>
							<ccts:DataType>Identifier. Type</ccts:DataType>
						</ccts:Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="cbc:TaxableAmount">
				<xsd:annotation>
					<xsd:documentation>
						<ccts:Component>
							<ccts:ComponentType>BBIE</ccts:ComponentType>
							<ccts:DictionaryEntryName>Base imponible percepcion
							</ccts:DictionaryEntryName>
							<ccts:Definition>Identifies the Consolidated Invoice Line.
							</ccts:Definition>
							<ccts:Cardinality>1</ccts:Cardinality>
							<ccts:ObjectClass>Consolidated Invoice Line</ccts:ObjectClass>
							<ccts:PropertyTerm>Identifier</ccts:PropertyTerm>
							<ccts:RepresentationTerm>Identifier</ccts:RepresentationTerm>
							<ccts:DataType>Identifier. Type</ccts:DataType>
						</ccts:Component>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- fin  Perception info in summary documents -->
	
	<xsd:complexType name="SUNATDeductionsType">
		<xsd:sequence>
			<xsd:element ref="SUNATReferenceValue" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="SUNATAquaticResources" minOccurs="0"
				maxOccurs="unbounded" />
			<xsd:element ref="SUNATGoldTrading" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATAquaticResourcesType">
		<xsd:sequence>
			<xsd:element ref="cbc:Description" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:StreetName" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cbc:DeliveryDate" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:MaritimeTransport" minOccurs="0"
				maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATGoldTradingType">
		<xsd:sequence>
			<xsd:element ref="cbc:Indicator" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:Percent" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:DescriptionCode" minOccurs="0"
				maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATReferenceValueType">
		<xsd:sequence>
			<xsd:element ref="cbc:Amount" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="SUNATValueTravel" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATValueTravelType">
		<xsd:sequence>
			<xsd:element ref="cbc:Amount" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:ReturnabilityIndicator" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:OriginAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="cac:DeliveryAddress" minOccurs="0"
				maxOccurs="1" />
			<xsd:element ref="SUNATValueVehicle" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATValueVehicleType">
		<xsd:sequence>
			<xsd:element ref="cbc:Quantity" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="cbc:Amount" minOccurs="0" maxOccurs="1" />
			<xsd:element ref="SUNATTransport" minOccurs="0"
				maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SUNATTransportType">
		<xsd:sequence>
			<xsd:element ref="cbc:TransportMeansTypeCode" minOccurs="0"
